<div>
  <h1>Plant Based Recipes</h1>
  <div>
    <%= button_to "Recipes", recipes_path, method: :get %>
  </div>
  <div>
    <%if current_user == nil%>
      <%= button_to "Sign In", login_path %>
    <%else%>
      <%= button_to "Log Out" , logout_path, method: :delete %>
    <%end%>
  </div>
  <div>
    <% if current_user != nil%>
      <%= button_to "Home", user_path(current_user), method: :get %>
    <%end%>
  </div>
</div>
